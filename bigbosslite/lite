#!/bin/bash


aaa="${1}"
echo -n 'file size: '
echo `du -h ${aaa} | sed "s#${aaa}##g"`
sed -i. '/Depiction: /d' ${1}
sed -i. '/homepage: /d' ${1}
sed -i. '/Website: /d' ${1}
sed -i. '/Installed-Size: /d' ${1}
sed -i. '/sileodepiction: /d' ${1}
sed -i. '/Sponsor: /d' ${1}
rm -rf ${1}.
echo -n 'now file size: '
echo `du -h ${aaa} | sed "s#${aaa}##g"`
